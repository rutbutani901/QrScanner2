<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.QrScanner2" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">?attr/greenAppColor</item>
        <item name="colorPrimaryVariant">?attr/greenAppColor</item>
        <item name="colorOnPrimary">?attr/greenAppColor</item>
        <item name="colorSecondary">?attr/greenAppColor</item>
        <item name="colorSecondaryVariant">?attr/greenAppColor</item>
        <item name="colorOnSecondary">?attr/greenAppColor</item>
        <item name="android:statusBarColor">@color/greenAppColor</item>
        <item name="android:includeFontPadding">false</item>
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
    </style>

    <style name="AppBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/AppModalStyle</item>
    </style>

    <style name="AppModalStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@drawable/shape_top_side_curves</item>
    </style>

    <style name="roundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="shapeableImgeViewStyle" parent="">
        <item name="cornerSize">@dimen/_10sdp</item>
    </style>
    <style name="shapeableImgeViewStyle5sdp" parent="">
        <item name="cornerSize">@dimen/_5sdp</item>
    </style>



    <style name="MyDialogStyle" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowMinWidthMinor">85%</item>
        <item name="android:windowMinWidthMajor">85%</item>

    </style>

    <style name="ThemeOverlay.App.MaterialAlertDialog" parent="ThemeOverlay.MaterialComponents.Dialog">
        <item name="android:windowMinWidthMajor">85%</item>
        <item name="android:windowMinWidthMinor">85%</item>

    </style>


    <style name="BottomNavigationViewTextStyle">
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>

    <style name="TimePickerTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/greenAppColor</item>
        <item name="colorPrimary">@color/greenAppColor</item>
        <item name="colorPrimaryDark">@color/greenAppColor</item>
        <item name="colorPrimaryVariant">@color/greenAppColor</item>
        <item name="colorOnPrimary">@color/greenAppColor</item>
        <item name="colorSecondary">@color/greenAppColor</item>

    </style>
    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/datePickerColor</item>
        <item name="android:buttonBarButtonStyle">@style/MyButtonBarButtonStyle</item>
    </style>
    <style name="MyButtonBarButtonStyle" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:textColor">@color/datePickerButtonsColor</item>
    </style>
    <style name="spinnerItemStyle" parent="android:Widget.TextView.SpinnerItem">
    </style>

    <style name="spinnerDropDownItemStyle" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
    </style>


